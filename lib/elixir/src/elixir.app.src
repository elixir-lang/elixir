{application, elixir,
[{description, "elixir"},
 {vsn, '$will-be-replaced'},
 {modules, '$will-be-replaced'},
 {registered, [elixir_sup, elixir_config, elixir_code_server]},
 {applications, [kernel,stdlib,compiler]},
 {mod, {elixir,[]}},
 {env, [
  {ansi_syntax_colors, [
    {atom, cyan},
    {binary, default_color},
    {boolean, magenta},
    {charlist, yellow},
    {list, default_color},
    {map, default_color},
    {nil, magenta},
    {number, yellow},
    {string, green},
    {tuple, default_color},
    {variable, light_cyan},
    {call, default_color},
    {operator, default_color}
  ]},
  {check_endianness, true},
  {dbg_callback, {'Elixir.Macro', dbg, []}},
  {time_zone_database, 'Elixir.Calendar.UTCOnlyTimeZoneDatabase'}
 ]}
]}.
